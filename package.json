{
    "name": "sevenk",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "cd create-react-app && npm run dev",
        "dev:backend": "cd backend && npm run dev",
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "cd create-react-app && npm run build",
        "build:backend": "cd backend && npm run build",
        "start": "npm run start:frontend",
        "start:frontend": "cd create-react-app && npm start",
        "start:backend": "cd backend && npm start"
    },
    "dependencies": {
        "@types/node": "^22.10.0",
        "@types/react": "^18.3.12",
        "@types/react-router-dom": "^5.3.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.6.3",
        "react-icons": "^5.3.0",
        "react-router-dom": "^7.0.1",
        "sequelize": "^6.35.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.7.2"
    }
}
